(module
	
	(import "../utility.watm")
	(import "../memory.watm")
	(import "../string.watm")
	(import "../list.watm")
	(import "../function.watm")
	(import "../object.watm")
	(import "../map.watm")
	(import "../number.watm")
	(import "../boolean.watm")
	(memory (import "host" "memory") 1)
	(import "host" "table" (table 1 anyfunc))
	(global $global (import "host" "global") (mut i32))
	
	(func $object_test_prototype (result i32)
		
		(local $global i32)
		(local $map i32)
		(set_local $global (get_global $global))
		(if (i32.eq (get_global $global) (i32.const 0)) (then
			(set_global $global (call $map_new))
			(set_local $global (get_global $global))
		))
		(set_local $map (call $map_get (get_local $global) (string "test")))
		(if (i32.eq (get_local $map) (i32.const 0)) (then
			(set_local $map (call $map_new))
			(call $map_set $global (string "test") (get_local $map))
			(call $map_set $map (string "test") (function "$object_test_test"))
		))
		(get_local $map)
	)
	
	(func $object_test_new (result i32)
		
		(local $object i32)
		(set_local $object (call $object_new))
		(call $object_subject_set (get_local $object) (get_local $object))
		(call $object_prototype_set (get_local $object) (call $object_test_prototype))
		(get_local $object)
	)
	
	(func $object_test_test (param $object i32) (result i32)
		(string "hello")
	)
)
